package testeEstagio;

public class teste2 {

	public static void main(String[] args) {
		   
		      int p, n, s;
		      p = 2749;
		      n = 8;
		      s = 50;
		      double pessoas = p/n;
		      double resultado = Math.round(pessoas);
		      resultado *= s;
		      double minuto = resultado+1;
		      minuto = Math.round(minuto/60*100);
		      System.out.println(minuto/100);
		    
		    }
}
